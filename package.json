{
	"name": "poe-idol-planner",
	"version": "0.5.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "react-router dev",
		"build": "react-router build",
		"test": "vitest run",
		"test:watch": "vitest",
		"data:convert": "tsx scripts/poedb-converter/index.ts",
		"data:convert:cached": "tsx scripts/poedb-converter/index.ts --cached",
		"data:convert:all": "tsx scripts/poedb-converter/index.ts --locales en,zh-TW,zh-CN,ko,ja,ru,pt-BR,de,fr,sp",
		"data:scarabs": "tsx scripts/scarab-converter/index.ts",
		"data:scarabs:cached": "tsx scripts/scarab-converter/index.ts --cached",
		"data:leagues": "tsx scripts/league-fetcher/index.ts",
		"deploy:dev:cf": "react-router build --mode worker-dev && wrangler deploy -e=worker-dev",
		"deploy:prod:cf": "react-router build --mode worker-prod && wrangler deploy -e=worker-prod",
		"biome": "biome check --write --unsafe",
		"biome:ci": "biome ci",
		"types": "pnpm run types:cf && pnpm run types:rr && pnpm run types:tsc",
		"types:cf": "wrangler types --env-interface CloudflareBindings",
		"types:rr": "react-router typegen",
		"types:tsc": "tsc -b",
		"postinstall": "pnpm run types:cf",
		"preview": "pnpm run build && vite preview",
		"precommit": "pnpm run biome:ci && pnpm run types",
		"prepare": "husky"
	},
	"dependencies": {
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-context-menu": "^2.2.16",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-popover": "^1.1.15",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slider": "^1.3.6",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tailwindcss/typography": "^0.5.19",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"isbot": "^5.1.33",
		"ky": "^1.14.2",
		"lucide-react": "^0.563.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-markdown": "^10.1.0",
		"react-router": "7.12.0",
		"remix-themes": "^2.0.4",
		"simple-icons": "^16.6.1",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.12",
		"@changesets/cli": "^2.29.8",
		"@cloudflare/vite-plugin": "^1.21.2",
		"@react-router/dev": "7.12.0",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.2",
		"@types/node": "^24.10.9",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"husky": "^9.1.7",
		"jsdom": "^27.4.0",
		"linkedom": "^0.18.12",
		"nanoid": "^5.1.6",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-tsconfig-paths": "^6.0.5",
		"vitest": "^4.0.18",
		"wrangler": "^4.60.0"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": "^24.13.0",
			"onFail": "download"
		},
		"packageManager": {
			"name": "pnpm",
			"version": "^10.28.1",
			"onFail": "warn"
		}
	},
	"packageManager": "pnpm@10.28.1",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@parcel/watcher",
			"esbuild",
			"sharp",
			"workerd"
		]
	}
}
